<!DOCTYPE html>
<html>
<head>
<%= link_to 'Back', estimates_summaries_path %>
</head>

<body>

<h1>ACS Estimates Summary</h1>

<table>
<col width="30">
<col width="30">
<col width="30">
<col width="30">
<col width="30">
<col width="30">
<col width="30">
<col width="30">
<col width="30">
<col width="120">
<tr>
    <th>FILEID</th>
    <th>FILETYPE</th>
    <th>SEQUENCE</th> 
    <th>CHARITER</th>
    <th>STUSAB</th>
    <th>LOGRECNO</th>
    <th>UA</th>
    <th>STATE</th>
    <th>ZIP</th>
    <th>GEOID</th>
    <th>NAME</th>
    <th colspan="6"></th>
  </tr>
 
  <% @estimates_summaries.each do |estimates_summary| %>
    <tr>
      <td><%= estimates_summary.FILEID %></td>
      <td><%= estimates_summary.FILETYPE %></td>
      <td><%= estimates_summary.SEQUENCE %></td>
      <td><%= estimates_summary.CHARITER %></td>
      <td><%= estimates_summary.STUSAB %></td>
      <td><%= estimates_summary.LOGRECNO %></td>
      <td><%= estimates_summary.UA %></td>
      <td><%= estimates_summary.STATE %></td>
      <td><%= estimates_summary.ZIPCODE %></td>
      <td><%= estimates_summary.GEOID %></td>
      <td><%= estimates_summary.NAME.encode("UTF-8", 'binary', invalid: :replace, undef: :replace, replace: '') %></td>
    </tr>
  <% end %>
</table>

</body>

</html>

